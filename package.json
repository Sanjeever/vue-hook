{
  "name": "@sanjeever/vue-hook",
  "publishConfig": {
    "access": "public"
  },
  "version": "1.0.2",
  "description": "Collection of essential Vue Composition Utilities",
  "author": "Sanjeev",
  "license": "MIT",
  "repository": "git@github.com:Sanjeever/vue-hook.git",
  "keywords": [
    "vue",
    "vue-hook"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "eslint": "^8.49.0",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.6",
    "unbuild": "^1.2.1"
  }
}
